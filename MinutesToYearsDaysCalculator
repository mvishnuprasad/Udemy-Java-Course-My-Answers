public class MinutesToYearsDaysCalculator {
  public static void printYearsAndDays(long minutes){
      long hours1=minutes%60;
      long min=minutes-hours1;
      long hours=min/60;
      long hourss=hours%24;
      long days1=hours%24;
      long hrs=hours-days1;
      long days=hrs/24;
      long dayss=days%365;
      long years1=days%365;
      long dys=days-years1;
      long years=dys/365;
      if(minutes<0){
          System.out.println("Invalid Value");
      }
      else{
          System.out.println(minutes+" min = "+years+" y and "+dayss+" d");
      }
  }
}
